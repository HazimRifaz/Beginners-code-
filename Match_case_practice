command = str(input("What programming language would you like to learn?"))
match command:
    case 'JavaScript':
        print ("You can become a web developer")
    case 'Python':
        print ("You can be a data scientist")
    case 'PHP'
        print ("You can become a backend developer")
    case 'Solidity'
        print ("You can become a blockchain developer")
    case 'Java'
        print ("You can become a modile app developer")
    case 'Nothing'
        print ("The language does not matter. What matters is solving problems. ")
